{
  "name": "tmh-web3",
  "version": "1.0.0",
  "description": "The Meeting House Website",
  "main": "index.js",
  "engines": {
    "node": ">=16.14.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "VERSION=`sentry-cli releases propose-version`;echo export const version ={git:\\\"$VERSION\\\"}>src/version.js;npx prettier -w src/version.js;HTTPS=true PORT=3006 react-scripts start",
    "start3": "react-scripts start",
    "build": "export SENTRY_ORG=the-meeting-house;VERSION=`sentry-cli releases propose-version`;sentry-cli releases new -p the-meeting-house $VERSION;sentry-cli releases set-commits --auto $VERSION;echo export const version ={git:\\\"$VERSION\\\"}>src/version.js;npx prettier -w src/version.js;react-scripts build;sentry-cli releases finalize $VERSION;sentry-cli releases deploys $VERSION new -e prod;",
    "test": "react-scripts test ",
    "publish": "COMMIT=`git rev-parse --short HEAD`;amplify publish -c;curl --header 'Content-Type: application/json' --request POST --data '{\"content\":\"Published a new version from commit: '$COMMIT'\"}' $DISCORD_WEBHOOK_WEB",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "testPayments": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https://localhost:3006 cy:open",
    "amplify:themeetinghousetmhShared": "cd amplify/backend/function/themeetinghousetmhShared/lib/nodejs && tsc && cd -",
    "amplify:cognitodevtmhPreTokenGeneration": "cd amplify/backend/function/cognitodevtmhPreTokenGeneration/src && tsc && cd -",
    "amplify:cognitodevtmhPostAuthentication": "cd amplify/backend/function/cognitodevtmhPostAuthentication/src && tsc && cd -",
    "amplify:tmhPinpointCreateCampaign": "cd amplify/backend/function/tmhPinpointCreateCampaign/src && tsc && cd -",
    "amplify:tmhClearCache": "cd amplify/backend/function/tmhClearCache/src && tsc && cd -",
    "amplify:tmhPinpointListSegments": "cd amplify/backend/function/tmhPinpointListSegments/src && tsc && cd -",
    "amplify:tmhPinpointCleanupEndpoints": "cd amplify/backend/function/tmhPinpointCleanupEndpoints/src && tsc && cd -",
    "amplify:tmhF1HomeChurch": "cd amplify/backend/function/tmhF1HomeChurch/src && tsc && cd -",
    "amplify:tmhStripeAddCustomer": "cd amplify/backend/function/tmhStripeAddCustomer/src && tsc && cd -",
    "amplify:tmhStripeAddPaymentMethod": "cd amplify/backend/function/tmhStripeAddPaymentMethod/src && tsc && cd -",
    "amplify:tmhStripeAddSubscription": "cd amplify/backend/function/tmhStripeAddSubscription/src && tsc && cd -",
    "amplify:tmhStripeAddPayment": "cd amplify/backend/function/tmhStripeAddPayment/src && tsc && cd -",
    "amplify:tmhStripeDeletePaymentMethod": "cd amplify/backend/function/tmhStripeDeletePaymentMethod/src && tsc && cd -",
    "amplify:tmhStripeDeleteSubscription": "cd amplify/backend/function/tmhStripeDeleteSubscription/src && tsc && cd -",
    "amplify:tmhStripeListPaymentMethods": "cd amplify/backend/function/tmhStripeListPaymentMethods/src &&  tsc && cd -",
    "amplify:tmhStripeListSubscriptions": "cd amplify/backend/function/tmhStripeListSubscriptions/src &&  tsc && cd -",
    "amplify:tmhF1LinkUser": "cd amplify/backend/function/tmhF1LinkUser/src &&  tsc && cd -",
    "amplify:tmhF1SearchContributionReceipts": "cd amplify/backend/function/tmhF1SearchContributionReceipts/src && tsc && cd -",
    "amplify:tmhF1SyncGroupPermissions": "cd amplify/backend/function/tmhF1SyncGroupPermissions/src && tsc && cd -",
    "amplify:tmhF1AssignmentSync": "cd amplify/backend/function/tmhF1AssignmentSync/src && tsc && cd -",
    "amplify:themeetinghousea5cd4619": "cd amplify/backend/function/themeetinghousea5cd4619/src && rm -rf node_modules/sharp && SHARP_IGNORE_GLOBAL_LIBVIPS=1 npm install --arch=x64 --libc=glibc --platform=linux sharp",
    "backup": "export z=$(date +%Y%m%d%H%M%S); mkdir backup/$z; cd backup/$z; dynamodump export-all-data --region=us-east-1 --profile tmhprodnew",
    "images": "mkdir ./public/static/images-sized;batch-image-resizer -i ./public/static/images -o ./public/static/images-sized -s '[50, 500, 1000, 1500]'",
    "createTestList": "find . -exec echo -n '\"{}\" ', \\; | sed 's/.json//g' | sed 's|./||g'",
    "syncProd": "for z in $(find ./public/static -name \"*.json\"); do npx jsonlint $z -q; if [ $? -eq 1 ] ; then echo $z; exit;fi; done; aws s3 sync ./public/static s3://heeetingouse-20190312104205-hostingbucket-tmhprod/static ;  aws cloudfront create-invalidation --distribution-id EYO80SQB3QGL --paths '/static/*' --profile amplify",
    "syncBeta": "for z in $(find ./public/static -name \"*.json\"); do npx jsonlint $z -q; if [ $? -eq 1 ] ; then echo $z; exit;fi; done; aws s3 sync ./public/static s3://heeetingouse-20190312104205-hostingbucket-dev/static ; aws cloudfront create-invalidation --distribution-id E57FJUWPGBGGI --paths '/static/*' --profile amplify",
    "syncProdDry": "for z in $(find ./public/static -name \"*.json\"); do npx jsonlint $z -q; if [ $? -eq 1 ] ; then echo $z; exit;fi; done; aws s3 sync ./public/static s3://heeetingouse-20190312104205-hostingbucket-tmhprod/static --dryrun ;  aws cloudfront create-invalidation --distribution-id EYO80SQB3QGL --paths '/static/*' --profile amplify",
    "syncBetaDry": "for z in $(find ./public/static -name \"*.json\"); do npx jsonlint $z -q; if [ $? -eq 1 ] ; then echo $z;exit; fi; done; aws s3 sync ./public/static s3://heeetingouse-20190312104205-hostingbucket-dev/static --dryrun; aws cloudfront create-invalidation --distribution-id E57FJUWPGBGGI --paths '/static/*' --profile amplify",
    "syncTest": "for z in $(find ./public/static -name \"*.json\"); do npx jsonlint $z -q; if [ $? -eq 1 ] ; then echo $z; exit;fi; done;"
  },
